<application>
  <component name="CodeInsightSettings">
    <option name="AUTOINSERT_PAIR_QUOTE" value="false" />
    <option name="REFORMAT_ON_PASTE" value="1" />
    <option name="INDENT_TO_CARET_ON_PASTE" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="10" />
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Menlo" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Python" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="TOOLTIPS_DELAY_MS" value="300" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern pattern="\b(todo\b.+@\w+)\b.*" />
    <pattern pattern="\b(todo @{0})\b.*" />
    <filter name="Authored">
      <pattern index="1" />
    </filter>
    <filter name="Un-Authored">
      <pattern index="2" />
    </filter>
  </component>
</application>