  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="LiveEditOptions" chromeAutoDelay="250" chromeUpdateJSOnChanges="true" nodeAutoDelay="1000" restartIfHotSwapFailed="true" updateFilesToCompile="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern pattern="\b(todo\b.+@\w+)\b.*" />
    <pattern pattern="\b(todo @{0})\b.*" />
    <filter name="Authored">
      <pattern index="1" />
    </filter>
    <filter name="Un-Authored">
      <pattern index="2" />
    </filter>
  </component>
