    <option name="REFORMAT_ON_PASTE" value="1" />
    <option name="INDENT_TO_CARET_ON_PASTE" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="500" />
    <option name="NEXT_ERROR_ACTION_GOES_TO_ERRORS_FIRST" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Menlo" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Python" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="TOOLTIPS_DELAY_MS" value="300" />
